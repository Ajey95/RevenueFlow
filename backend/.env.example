# .env.example - Environment Configuration for RevenueFlow Backend

# Server Configuration
NODE_ENV=development
PORT=3001
FRONTEND_URL=http://localhost:5173

# Groq API Configuration (REQUIRED)
GROQ_API_KEY=your_groq_api_key_here

# Fetch.ai Agent Configuration (REQUIRED)
LEAD_AGENT_SEED=lead_agent_secret_seed_12345
DEAL_AGENT_SEED=deal_agent_secret_seed_67890
REVENUE_AGENT_SEED=revenue_agent_secret_seed_54321

# Fetch.ai Network Configuration
FETCHAI_NETWORK=testnet
FETCHAI_CHAIN_ID=dorado-1

# Vultr Configuration (for deployment)
VULTR_API_KEY=your_vultr_api_key
VULTR_REGION=ewr
VULTR_PLAN=vc2-1c-1gb

# Database Configuration (optional for advanced features)
DATABASE_URL=postgresql://username:password@localhost:5432/revenueflow
REDIS_URL=redis://localhost:6379

# Security Configuration
JWT_SECRET=your_jwt_secret_here
ENCRYPTION_KEY=your_encryption_key_here

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Logging Configuration
LOG_LEVEL=info
LOG_FILE=logs/revenueflow.log

# External Integrations (optional)
SLACK_WEBHOOK_URL=your_slack_webhook_url
EMAIL_SERVICE_API_KEY=your_email_service_key

# Demo Configuration
DEMO_MODE=true
MOCK_DATA=true

# AI Model Configuration
DEFAULT_MODEL=llama-3.1-70b-versatile
FALLBACK_MODEL=llama-3.1-8b-instant
MAX_TOKENS=1024
TEMPERATURE=0.7

# Agent Communication
AGENT_NETWORK_PORT=8000
AGENT_DISCOVERY_INTERVAL=30000
AGENT_HEALTH_CHECK_INTERVAL=60000

# Performance Monitoring
METRICS_ENABLED=true
METRICS_PORT=9090
HEALTH_CHECK_INTERVAL=30000